{
  "name": "constitutional-market-harmonics",
  "version": "1.0.0",
  "description": "Chaos-theoretic trading system optimizing financial returns and constitutional impact using strange attractors",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "trading:loop": "node dist/index.js --trading-loop",
    "backtest": "node dist/index.js --backtest",
    "dashboard": "node dist/index.js --dashboard"
  },
  "keywords": [
    "trading",
    "chaos-theory",
    "constitutional-ai",
    "quantitative-finance",
    "portfolio-optimization",
    "strange-attractors",
    "ethical-investing"
  ],
  "author": "Axiom-X",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "mathjs": "^12.0.0",
    "numeric": "^1.2.6",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "sqlite3": "^5.1.0",
    "ws": "^8.14.0",
    "yargs": "^17.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/mathjs": "^9.4.2",
    "@types/node": "^20.0.0",
    "@types/sqlite3": "^3.1.8",
    "@types/ws": "^8.5.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}